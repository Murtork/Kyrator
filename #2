al='АКОР'
i=1
k=0
for x1 in al:
    for x2 in al:
        for x3 in al:
            for x4 in al:
                for x5 in al:
                    s=x1+x2+x3+x4+x5
                    if (s.count('А')+s.count('О'))==1 and i%2==0:
                        k=k+1
                    if i%2==1:
                        k=k+1
                    i=i+1
print(k)
